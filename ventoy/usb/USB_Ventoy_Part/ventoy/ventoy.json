{
    "control": [
        { "VTOY_DEFAULT_MENU_MODE": "1" },
        { "VTOY_FILT_DOT_UNDERSCORE_FILE": "1" },
        { "VTOY_TREE_VIEW_MENU_STYLE": "1" }
    ],
    
    "theme": {
        "file": "/ventoy/theme/theme.txt",        
        "gfxmode": "1366x768",    
        "display_mode": "GUI",
        "ventoy_left": "5%",
        "ventoy_top": "95%",
        "ventoy_color": "#ffffff"
    },
    
    "menu_class": [
        {
            "key": "acronis",
            "class": "acronis"
        },        
        {
            "key": "linuxmint",
            "class": "linuxmint"
        },
        {
            "key": "o_o",
            "class": "o_o"
        },        
        {
            "key": "ubuntu",
            "class": "ubuntu"
        },
        {
            "key": "windows",
            "class": "windows"
        },
        {
            "key": "memtest86",
            "class": "memtest86"
        },
        {
            "dir": "/Acronis Utilities",
            "class": "folder"
        },
        {
            "dir": "/Hardware Testers",
            "class": "folder"
        },
        {
            "dir": "/Install Linux",
            "class": "folder"
        },
        {
            "dir": "/Install Windows",
            "class": "folder"
        },
        {
            "dir": "/Live OS",
            "class": "folder"
        },
        {
            "dir": "/Untested Tools",
            "class": "folder"
        }
    ],
    
    "menu_alias": [
        {
            "image": "/Acronis Utilities/acronis-cyber_backup_v12.5.iso",
            "alias": "Acronis Cyber Backup v12.5"
        },
        {
            "image": "/Acronis Utilities/acronis-ti_dd_ur.iso",
            "alias": "Acronis TI 2020/DD 12.5/UR 11.7"
        },
        {
            "image": "/Hardware Testers/memtest86-usb.img",
            "alias": "Memtest86 9.3"
        },
        {
            "image": "/Install Linux/linuxmint-20.2-xfce-64bit.iso",
            "alias": "Linux Mint 20.2 XFCE"
        },
        {
            "image": "/Install Windows/windows-10.iso",
            "alias": "Windows 10 21h2 x64"
        },
        {
            "image": "/Install Windows/windows-10ltsc.iso",
            "alias": "Windows 10 LTSC 21h2 x64"
        },
        {
            "image": "/Live OS/windows_DaRT10_x64.iso",
            "alias": "DaRT Windows 10 x64"
        },
        {
            "image": "/Live OS/windows_Win10XPE_x64.ISO",
            "alias": "Windows 10XPE Lite x64"
        },
        {
            "image": "/Live OS/o_o_BlueCon_v18.0.8088.wim",
            "alias": "O&O BlueCon v18.0"
        }
    ],
    
    "auto_install": [
        {
            "image": "/Install Windows/windows-10.iso",
            "template": [
			"/ventoy/script/autounattend-dragon.xml",
			"/ventoy/script/autounattend-home.xml"],
            "autosel": 0,
            "timeout": 10
        },
        {
            "image": "/Install Windows/windows-10ltsc.iso",
            "template": "/ventoy/script/autounattend_ltsb.xml"
        }
    ],
	
    "auto_memdisk": [
        "/Hardware Testers/memtest86-usb.img",
        "/ISO/ESXi.iso",
        "/ISO/Win10PE.iso"
    ]
    
}
